                        @*<a href="@dx.Rows[0]["com_Twitter"].ToString()" target="_blank"><img src="../content/imgs/theme/icons/icon-twitter-white.svg" alt="" /></a>*@
                        <a href="@dx.Rows[0]["com_Instagram"].ToString()" target="_blank"><img src="../content/imgs/theme/icons/icon-instagram-white.svg" alt="" /></a>
