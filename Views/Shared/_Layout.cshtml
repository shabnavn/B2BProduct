                                foreach (DataRow dr in popdts.Rows)
                                {
                                    <li><a onclick="Search('@dr["cat_ID"].ToString()', '0','@dr["cat_Name"].ToString()' , '' )">@dr["cat_Name"].ToString()</a></li>
                                }
