                            <li><a href="/Home/Privacy">Privacy Policy</a></li>
                            <li><a href="/Home/Terms">Terms &amp; Conditions</a></li>
                            <li><a href="/Support/NewSupport">Support</a></li>
                        </ul>
                    </div>
                    <div class="footer-link-widget col wow animate__animated animate__fadeInUp">
                        <h4 class="widget-title">Account</h4>
                        <ul class="footer-list mb-sm-5 mb-md-0">
                            <li><a href="/Home/Cart">View Cart</a></li>
                            @{
                                if (Roles.Contains("AD") || Roles.Contains("FN") || Roles.Contains("OT"))
                                {
                                    <li><a href="/Order/AllOrders">Orders</a></li>
                                }
                            }

                            @*<li><a href="/Promotion/AllPromo">Promotions</a></li>*@
                        </ul>
