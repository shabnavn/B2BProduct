    <script>
        $(".customer-link").click(function (e) {
            e.preventDefault();

            // Get the customer ID from the data-customer-id attribute
            var customerId = $(this).data("customer-id");
            var cusName = $(this).data("customer");
            var customerName = $(this).text(); // Get the customer name

            // Set the selected customer's name in the span
            $("#selectedCustomerName").text(customerName);


            // Set the customer ID in TempData
            $.ajax({
                type: "POST",
                url: "/Profile/SetSelectedCustomerId",
                data: { customerId: customerId, cusName: cusName },
                success: function () {
                    // Redirect to the other page after setting TempData
                    window.location.href = "/Profile/Account";
                }
            });
        });
    </script>

